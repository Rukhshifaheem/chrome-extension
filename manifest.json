{
  "manifest_version": 3,
  "name": "New Tab Customizer",
  "version": "1.0",
  "description": "Customize your new tab with a personalized greeting, favorite links, and more.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  }
}
